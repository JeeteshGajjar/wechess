{
  "name": "wechess",
  "version": "0.0.1",
  "scripts": {
    "lint1": "eslint ./lib ./client ./server --ignore-pattern **/*.min.js  --ext .js --ext .jsx",
    "lint": "eslint ./lib ./client ./server --ignore-path .eslintignore  --ext .js --ext .jsx",
    "lintorg": "eslint ./libs ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx",
    "dsstore": "find . -name '*.DS_Store' -type f -delete",
    "translate": "babel-node .scripts/translate.js",
    "start": "meteor --settings settings-development.json",
    "deploy": "modulus env set METEOR_SETTINGS \"$(cat settings.json)\" -p 'wechess' && modulus deploy -p 'wechess'"
  },
  "devDependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-plugin-react-require": "2.x.x",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "2.x.x",
    "eslint": "1.10.x",
    "eslint-plugin-react": "3.15.x",
    "mocha": "2.x.x",
    "react-addons-test-utils": "^15.1.0",
    "sinon": "1.17.x"
  },
  "dependencies": {
    "chess.js": "^0.10.2",
    "domready": "^1.0.8",
    "formsy-material-ui": "^0.4.3",
    "formsy-react": "^0.18.0",
    "is_js": "^0.8.0",
    "mantra-core": "^1.6.1",
    "material-ui": "^0.15.0",
    "react": "^15.1.0",
    "react-copy-to-clipboard": "^4.1.0",
    "react-dom": "^15.1.0",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "private": true
}
